{
    "manifest_version": 3,
    "name": "WebNotes",
    "description": "WebNotes client side handler",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/logo.png"
    },
    
    "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://canvas.nau.edu/*"
      ]
    }
  ]
  }